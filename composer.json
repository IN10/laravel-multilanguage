{
    "name": "in10/laravel-multilanguage",
    "description": "Simple, opinionated multilanguage package for Laravel",
    "type": "library",
    "require": {
        "php": "^7.3",
        "laravel/framework": "^5.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jakob Buis",
            "email": "jakob@jakobbuis.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "IN10\\Multilanguage\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "IN10\\Multilanguage\\ServiceProvider"
            ]
        }
    }
}
